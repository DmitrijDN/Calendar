
<div class="row team-header" data-bind="foreach: userColumns">
    <div data-bind="text: title"></div>
</div>
        
<div class="row team-column" data-bind="foreach: userColumns">
    <div data-bind="template: { name: 'team-template', foreach: allUsers }"></div>
</div>
        
<script type="text/html" id="team-template">
    <div class="row">
        <span data-bind="text : $root.shortName(FullName)"></span>
        <i data-bind="click : function() { $root.addUser($parent, $data)}" class="icon-plus-circled pull_right"></i>
    </div>
</script>